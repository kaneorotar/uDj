            <div class="ring">
            </div>
            <div class="ring2">
            </div>
            <div class="ring3">
            </div>
            <div class="ring4">
            </div>
            <div class="ring5">
            </div>
            <div class="ring6">
            </div>
            <div class="ring7">
            </div>
            <div class="inner">
            </div>
            <div class="dot">
            </div>
           </div>
        </div>
      </div>
    <button type="button" id="playButton" onclick="pauseAnim()">Click Me!</button>
    <div class="section">
      <div class="current-song section">
        <h4>Style - Taylor Swift - 1989</h4>
      </div>
    </div>
    <div class="section queue">
      <h3>Queue</h3>
      <div class="song-list">
        <div class="song-box">
          
        </div>
        <div class="song-box">
          
        </div>
        <div class="song-box">
          
        </div>
        <div class="song-box">
          
        </div>
